{
    "name": "Don't track me YouTube",
    "description": "Removes tracking for URLs redirections on YouTube inspired by the extension : Don't track me Google.",
    "version": "0.1",
    "manifest_version": 3,
    "icons": {
        "48": "icons/icon48.png",
        "96": "icons/icon96.png"
    },
    "content_scripts": [
        {
            "run_at": "document_idle",
            "js": [
                "src/main.js"
            ],
            "matches": [
                "*://*.youtube.com/*"
            ]
        }
    ]
}